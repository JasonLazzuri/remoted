# Remoted Environment Configuration
# Copy this file to .env and update with your values

# Signaling Server URL (WebSocket)
# Development: ws://localhost:8080
# Production: wss://your-signaling-server.com
SIGNALING_SERVER=ws://localhost:8080

# TURN Server Configuration (for NAT traversal)
# Leave empty for development, required for production cross-network
TURN_SERVER_URL=
TURN_SERVER_USERNAME=
TURN_SERVER_CREDENTIAL=

# Environment
NODE_ENV=development
